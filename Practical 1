def binary_search(arr, target):
    low = 0
    high = len(arr) - 1

    while low <= high:
        mid = (low + high) 

        if arr[mid] == target:
            return mid
        elif arr[mid] < target:
            low = mid + 1
        else:  
            high = mid - 1

    return -1  

arr = [2,3,4,5,6,7]
target = int(input("Enter number to search: "))

output - Enter number to search:  7
